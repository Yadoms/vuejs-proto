{
  "name": "widget1",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://abhay07.github.io/movieAppAngular",
  "scripts": {
    "widgetgeneric": "vue-cli-service build --target lib --formats umd-min --no-clean --dest public/widgets/widget1 --name \"MyComponent.[chunkhash]\" widget.vue",
    "widget1": "vue-cli-service build --target lib --formats umd-min --no-clean --dest ../public/widgets/widget1 --name \"widget1\" widget.vue",
    "widget2": "vue-cli-service build --target lib --formats umd-min --no-clean --dest ../public/widgets/widget2 --name \"widget2\" widget2.vue",
    "widget3": "vue-cli-service build --target lib --formats umd-min --no-clean --dest ../public/widgets/widget3 --name \"widget3\" widget3.vue",
    "allwidgets": "npm run widget1 && npm run widget2 && npm run widget3"
  },
  "dependencies": {
    "highcharts": "^7.2.1",
    "highcharts-vue": "^1.3.5",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.1",
    "@vue/cli-plugin-eslint": "^4.1.1",
    "@vue/cli-service": "^5.0.8",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.0.1",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
